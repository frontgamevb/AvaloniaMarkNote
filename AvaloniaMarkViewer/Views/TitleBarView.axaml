<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:tr="clr-namespace:Localization.Avalonia;assembly=Localization.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaMarkViewer.Views.TitleBarView">
	<DockPanel>
		<Menu>
			<MenuItem Header="{tr:Localize FileMenuItem}">
				<MenuItem Name="ExitMenuItem" Header="{tr:Localize ExitMenuItem}" />
			</MenuItem>
		</Menu>
	</DockPanel>
</UserControl>